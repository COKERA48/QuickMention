<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.CSC481Project.ashley.quickmentiontest">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.CSC481Project.ashley.quickmentiontest.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/qm_launch_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/qm_launch_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="com.CSC481Project.ashley.quickmentiontest.SignIn">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.CSC481Project.ashley.quickmentiontest.SignUp" />
        <activity android:name="com.CSC481Project.ashley.quickmentiontest.MainActivity" />
    </application>

</manifest>